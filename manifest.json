{
    "manifest_version": 2,
    "name": "Session Saver",
    "description": "Group tabs together, like nature intended",
    "version": "1.1",
    "browser_action": {
        "default_title": "In class",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "Snail-16.png"
        }
    },
    "icons": { 
        "16": "Snail-16.png",
        "48": "Snail-48.png",
        "128": "Snail-128.png" 
    },
    "permissions": [
        "tabs",
        "sessions",
        "activeTab",
        "storage"
    ]
}
